{
    "extends": "airbnb",
    "rules": {
      "semi": ["error", "never"],
      "import/no-unresolved": "off",
      "indent": ["error", "tab", { "SwitchCase": 1 }],
      "max-len": "off",
      "no-shadow": ["error", { "allow": ["resolve", "reject", "done", "cb", "e", "err", "error"] }],
      "no-underscore-dangle": ["error"],
      "new-cap": ["error"],
      "newline-before-return": "error"
    }
}